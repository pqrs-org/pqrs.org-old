<div class="container">
  <section>
    <div class="page-header">
      <h1>
        Version History
      </h1>
    </div>
    {{#histories}}
      <h2>
        {{version}}
        {{#updated}}
          <small>
            {{#format_date}}
              {{updated}}
            {{/format_date}}
          </small>
        {{/updated}}

        {{#link}}
          <a class="btn btn-default" href="{{link}}">
            Download
          </a>
        {{/link}}
      </h2>
      <div style="margin-left: 2em">
        {{{body}}}
      </div>
    {{/histories}}
  </section>
</div>
