<div class="container">
  <section>
    <div class="page-header">
      <h1>
        Version History
      </h1>
    </div>
    {{#histories}}
      <h2>
        Version {{version}}
        {{#updated}}
          <small>
            {{#format_date}}
              {{updated}}
            {{/format_date}}
          </small>
        {{/updated}}
      </h2>
      {{{body}}}
    {{/histories}}
  </section>
</div>
