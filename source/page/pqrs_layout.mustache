<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>{{title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Bootstrap core CSS -->
    <link href="/assets/bootstrap/bootstrap-3.3.6/dist/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Documentation extras -->
    <link href="/assets/bootstrap/bootstrap-3.3.6/docs/assets/css/docs.min.css" rel="stylesheet" />
    <link href="{{#assets}}/css/output/pqrs.css{{/assets}}" rel="stylesheet" />
    <link href="{{#assets}}/css/output/sparkle.css{{/assets}}" rel="stylesheet" />

    <!-- Bootstrap extra -->
    {{#enable_prettify}}
      <link href="/assets/bootstrap-prettify/prettify.css" rel="stylesheet" type="text/css"/>
    {{/enable_prettify}}

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Favicons -->
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
    <link rel="manifest" href="/site.webmanifest" />
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="theme-color" content="#ffffff" />
    <meta name="twitter:card" content="summary" />
    {{#og_title}}
      <meta property="og:title" content="{{og_title}}" />
    {{/og_title}}
    {{#og_description}}
      <meta property="og:description" content="{{og_description}}" />
    {{/og_description}}
    {{#og_image}}
      <meta property="og:image" content="{{og_image}}" />
    {{/og_image}}
  </head>

  <body class="{{#body_classes}} {{class}} {{/body_classes}}">

    <!-- Docs master nav -->
    <header class="navbar navbar-fixed-top bs-docs-nav">
      <div class="container">
        <div class="navbar-header">
          <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>

          <a href="/index.html" class="navbar-brand">pqrs.org</a>
        </div>
        <nav class="collapse navbar-collapse bs-navbar-collapse">
          <ul class="nav navbar-nav">
            {{#navs_left}}
              {{#dropdown}}
                <li class="{{nav_class}}">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                    {{name}}
                    <b class="caret"></b>
                  </a>
                  <ul class="dropdown-menu">
                    {{#items}}
                      <li {{#dropdown_nav_class}}class="{{dropdown_nav_class}}"{{/dropdown_nav_class}}>
                        <a href="{{href}}">{{name}}</a>
                      </li>
                    {{/items}}
                  </ul>
                </li>
              {{/dropdown}}
              {{^dropdown}}
                <li {{#nav_class}}class={{nav_class}}{{/nav_class}}>
                  <a href="{{href}}">{{name}}</a>
                </li>
              {{/dropdown}}
            {{/navs_left}}
          </ul>
          <ul class="nav navbar-nav navbar-right">
            {{#navs_right}}
              <li {{#nav_class}}class={{nav_class}}{{/nav_class}}>
                <a href="{{href}}">{{name}}</a>
              </li>
            {{/navs_right}}
          </ul>
        </nav>
      </div>
    </header>

    {{{body}}}

    <footer class="bs-docs-footer">
      <div class="container">
        <a href="#">Back to top</a>
      </div>
    </footer>

    <script src="/assets/jquery/jquery-2.1.4.min.js"></script>
    <script src="/assets/bootstrap/bootstrap-3.3.6/dist/js/bootstrap.min.js"></script>
    {{#enable_prettify}}
      <script src="/assets/google-code-prettify/prettify.js" type="text/javascript"></script>
      <script type="text/javascript">jQuery(function(){ prettyPrint(); });</script>
    {{/enable_prettify}}
    {{#enable_jquery_collapse_js}}
      <script type="text/javascript" src="/assets/jquery/jquery.collapse.js"></script>
      <script type="text/javascript">jQuery(function(){ jQuery('#collapser').jqcollapse(); });</script>
    {{/enable_jquery_collapse_js}}
    <script src="{{#assets}}js/pqrs.js{{/assets}}"></script>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      var GA_LOCAL_STORAGE_KEY = 'ga:clientId';

      if (window.localStorage) {
        ga('create', 'UA-56715024-1', {
          'storage': 'none',
          'clientId': localStorage.getItem(GA_LOCAL_STORAGE_KEY)
        });
        ga(function(tracker) {
          localStorage.setItem(GA_LOCAL_STORAGE_KEY, tracker.get('clientId'));
        });
      } else {
        ga('create', 'UA-56715024-1', 'auto');
      }

      ga('send', 'pageview');
    </script>
  </body>
</html>
