<!DOCTYPE html>
<html lang="{{language}}">
  <head>
    <meta charset="utf-8" />
    <title>{{title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!-- Bootstrap core CSS -->
    <link href="/assets/bootstrap/bootstrap-3.3.6/dist/css/bootstrap.min.css" rel="stylesheet" />

    <!-- Documentation extras -->
    <link href="/assets/bootstrap/bootstrap-3.3.6/docs/assets/css/docs.min.css" rel="stylesheet" />
    <link href="/css/pqrs.css?1391687151" rel="stylesheet" />
    <link href="/css/sparkle.css?1452433405" rel="stylesheet" />

    <!-- Bootstrap extra -->
    {{#enable_prettify}}
      <link href="/assets/bootstrap-prettify/prettify.css" rel="stylesheet" type="text/css"/>
    {{/enable_prettify}}

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    <!-- Favicons -->
    <link rel="shortcut icon" href="/favicon.ico" />
  </head>

  <body>

    <!-- Docs master nav -->
    <header class="navbar navbar-static-top bs-docs-nav">
      <div class="container">
        <div class="navbar-header">
          <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>

          <a href="/index.html.{{language}}" class="navbar-brand">pqrs.org</a>
        </div>
        <nav class="collapse navbar-collapse bs-navbar-collapse">
          <ul class="nav navbar-nav">
            {{#navs_left}}
              {{#dropdown}}
                <li class="{{nav_class}}">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                    {{name_l10n}}
                    <b class="caret"></b>
                  </a>
                  <ul class="dropdown-menu">
                    {{#items}}
                      <li {{#dropdown_nav_class}}class="{{dropdown_nav_class}}"{{/dropdown_nav_class}}>
                        <a href="{{href}}.{{language}}">{{name_l10n}}</a>
                      </li>
                    {{/items}}
                  </ul>
                </li>
              {{/dropdown}}
              {{^dropdown}}
                <li {{#nav_class}}class={{nav_class}}{{/nav_class}}>
                  <a href="{{href}}.{{language}}">{{name_l10n}}</a>
                </li>
              {{/dropdown}}
            {{/navs_left}}
          </ul>
          <ul class="nav navbar-nav navbar-right">
            {{#navs_right}}
              <li {{#nav_class}}class={{nav_class}}{{/nav_class}}>
                <a href="{{href}}.{{language}}">{{name_l10n}}</a>
              </li>
            {{/navs_right}}
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                Language
                <b class="caret"></b>
              </a>
              <ul class="dropdown-menu">
                <li><a href="{{path}}.en">English</a></li>
                <li><a href="{{path}}.ja">Japanese</a></li>
              </ul>
            </li>
          </ul>
        </nav>
      </div>
    </header>

    {{{body}}}

    <footer class="bs-docs-footer">
      <div class="container">
        <ul class="bs-docs-footer-links muted">
          <li><a href="#">Back to top</a></li>
        </ul>
      </div>
    </footer>

    <script src="/assets/jquery/jquery-2.1.4.min.js"></script>
    <script src="/assets/bootstrap/bootstrap-3.3.6/dist/js/bootstrap.min.js"></script>
    {{#enable_prettify}}
      <script src="/assets/google-code-prettify/prettify.js" type="text/javascript"></script>
      <script type="text/javascript">jQuery(function(){ prettyPrint(); });</script>
    {{/enable_prettify}}
    {{#enable_jquery_collapse_js}}
      <script type="text/javascript" src="/assets/jquery/jquery.collapse.js"></script>
      <script type="text/javascript">jQuery(function(){ jQuery('#collapser').jqcollapse(); });</script>
    {{/enable_jquery_collapse_js}}
    <script src="/js/pqrs.js?1376532151"></script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-56715024-1', 'auto');
      ga('send', 'pageview');

    </script>
  </body>
</html>
