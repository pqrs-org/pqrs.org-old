    border-color: #337ab7;
}
.update-description-item-new .update-description-label:before {
    content: "new";
}
.update-description-item-important {
    border-color: #d9534f;
}
.update-description-item-important .update-description-label:before {
    content: "important";
}
.update-description-item-fixed {
    border-color: #5cb85c;
